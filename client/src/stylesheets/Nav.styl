@import 'palette.styl'

Nav
  display: flex
  flex-direction: row
  justify-content: left
  height: 5vh
  width: 10%
  margin: 0
  padding: 1vh

.nav-content
  display flex
  color white
  h1
    padding 1vh
    margin auto
    font-weight normal
    &:hover
      border 1px solid white
      -webkit-border-radius 5px
      -moz-border-radius 5px
      border-radius 5px
